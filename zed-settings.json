{
  "theme": "One Light",
  "base_keymap": "JetBrains",
  "buffer_font_size": 15,
  "buffer_font_features": {
    "calt": false
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 500
    }
  },
  "format_on_save": "off",
  "soft_wrap": "none",
  "max_tabs": 4,
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  }
}
